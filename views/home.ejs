                              <% if (feeds.length > 0) { %>
                                 <% feeds.forEach(f => {%>
                                   
                                    <span><h4 class="title" > <img src="<%= f.feedLogo %>" alt="#" style="width:30%" /></h4>
                                    </span>
                                    
                                    
                                    <p id = "feed-article"></p>
                                       <br/></br>
                                          <script>
                                          alert("in script");
                                           const url = '/getfeeds/<%= f._id %>';
                                           $.ajax({
                                                      url: url,
                                                      type : "GET",
                                                      data: { },
                                                      success : function(rss) {
                                                      //   Set up url
                                                      
                                                      var currentHTML = $("#feed-article").val();
                                                      
                                                         rss.items.forEach(item => {
                                                            let articleUrl="<div class='post-heading'>"+
                                                               "</div>"+
                                                               "<h3>"+item.title+"</h3>"+
                                                                 "<i>"+item.pubDate.substring(0, item.pubDate.length-6)+"</i></br>"+
                                                               // "<p>"+item.contentSnippet+"</p><br/>"+
                                                                  
                                                               
                                                               "<div class='full'><a  target='_blank' href="+item.link+"><button type='button' class='btn btn-success'>Read More</button></a><br/><br/>"
                                                            
                                                            

                                                            currentHTML+=articleUrl;
                                                         });
                                                         
                                                         
                                                         $("#feed-article").html(currentHTML);
                                                         
                                                         
                                                         
                                                         
                                                      },
                                                      error : function(e) {
                                                         console.error(`${e.name}: ${e.message}`);
                                                         alert("error"+error)
                                                      }
                                                   
                                          });
                                          </script>
                                          </br>
                                         

                                       </p>
                                 <% }) %>
                                 <% }else { %>
